{
  "name": "run-batch",
  "version": "0.0.1",
  "description": "npm run-script helper that allows running a bunch of run-scripts - in series or parallel!",
  "main": "index.js",
  "scripts": {
    "sample:1": "node ./scripts/colored.js -t 1001",
    "sample:2": "node ./scripts/simple.js -t 1002",
    "sample:3": "node ./scripts/simple.js -t 1003",
    "sample:4": "node ./scripts/colored.js -t 5004",
    "long-100": "node -e \"setTimeout(function() { console.log('long 100') }, 1000)\"",
    "long-120": "node -e \"setTimeout(function() { console.log('long 120') }, 1000)\"",
    "long-200": "node -e \"setTimeout(function() { console.log('long-200') }, 1000)\"",
    "series": "npm run long-100 && npm run long-120 && npm run long-200",
    "test": "mocha",
    "trial1": "node ./index.js",
    "trial2": "node ./index.js",
    "trial3": "node ./index.js",
    "trial4": "npm run sample:1",
    "trial5": "node ./index.js"
  },
  "run-batch": {
    "trial1": [
      "long-100",
      "long-120",
      "long-200"
    ],
    "trial2": [
      "sample:1",
      "sample:2",
      "sample:4"
    ],
    "trial3": [
      "sample:1",
      [
        "sample:2",
        "sample:4"
      ],
      "sample:3"
    ],
    "trial5": [
      "sample:1"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sramam/run-script-sequence.git"
  },
  "keywords": [
    "npm",
    "run",
    "script",
    "scripts",
    "sequence",
    "series",
    "parallel"
  ],
  "author": "Shishir Ramam",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sramam/run-script-sequence/issues"
  },
  "homepage": "https://github.com/sramam/run-script-sequence#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "mocha": "^3.0.2"
  },
  "dependencies": {
    "bluebird": "^3.4.3",
    "commander": "^2.9.0",
    "cross-spawn": "^4.0.0"
  }
}
